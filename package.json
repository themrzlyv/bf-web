{
  "name": "bf-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development && next dev",
    "build": "cross-env NODE_ENV=production && next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint",
    "test": "cross-env NODE_ENV=test && jest",
    "prisma:generate:dev": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "prisma:push:dev": "dotenv -e .env.development.local -- npx prisma db push",
    "prisma:generate:prod": "dotenv -e .env.production.local -- npx prisma generate",
    "prisma:migrate:prod": "dotenv -e .env.production.local -- npx prisma migrate deploy",
    "prisma:push:prod": "dotenv -e .env.production.local -- npx prisma db push",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv-cli": "^7.4.2",
    "lucide-react": "^0.437.0",
    "next": "14.2.7",
    "next-auth": "^5.0.0-beta.20",
    "patch-package": "^8.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-redux": "^9.1.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@prisma/client": "^5.19.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-config-next": "^14.2.7",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "postinstall-postinstall": "^2.1.0",
    "prisma": "^5.19.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.3.0"
  }
}
